<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff90caf9">
    <TextView
        android:text="History"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/text_title_history"
        android:textSize="80px"
        android:textAlignment="center"
        android:textColor="#000"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="10px"
        android:layout_marginTop="10px"
        android:background="#ff212121"/>
    <ListView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="407.6dp"
        android:id="@+id/listView1"
        android:layout_marginTop="50px"
        android:background="#ffeceff1"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"/>
    <Button
        android:text="Back"
        android:layout_width="160.5dp"
        android:layout_height="47.0dp"
        android:layout_marginLeft="350px"
        android:layout_marginTop="150px"
        android:id="@+id/btn_back"
        />
</LinearLayout>
